	Agent any
        stages {
                stage('build') {
                        steps {
                                sh 'Building application'
                        }
                
                     }

		          stage('test') {
                        steps {
                                sh 'test application'
                        }
                
                     }

		        stage('deploy') {
                        steps {
                                sh 'deploying application'
                        }
                
                     }

	}
       }
